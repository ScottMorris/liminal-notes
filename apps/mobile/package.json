{
  "name": "@liminal-notes/mobile",
  "version": "0.1.0",
  "main": "expo-router/entry",
  "scripts": {
    "build:editor": "pnpm --filter editor-web build",
    "prestart": "pnpm build:editor",
    "start": "expo start --dev-client",
    "android": "expo start --android --dev-client",
    "ios": "expo start --ios --dev-client",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "build:preview:android": "pnpm eas build --profile preview --platform android --local",
    "build:dev:android": "pnpm eas build --profile development --platform android --local",
    "build:dev:ios": "pnpm eas build --profile development --platform ios --local",
    "build:dev": "pnpm eas build --profile development --local"
  },
  "dependencies": {
    "@liminal-notes/core-shared": "workspace:*",
    "@liminal-notes/vault-core": "workspace:*",
    "@pchmn/expo-material3-theme": "^1.3.2",
    "@react-native-community/slider": "^5.0.1",
    "@react-navigation/native": "^7.1.26",
    "babel-preset-expo": "^54.0.9",
    "expo": "~54.0.30",
    "expo-constants": "^18.0.12",
    "expo-dev-client": "~6.0.20",
    "expo-file-system": "^19.0.21",
    "expo-linking": "^8.0.11",
    "expo-router": "^6.0.21",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-native": "0.81.5",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "^4.1.6",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.16.0",
    "react-native-webview": "13.15.0"
  },
  "devDependencies": {
    "@expo/ngrok": "^4.1.3",
    "@types/react": "~19.1.0",
    "eas-cli": "^16.28.0",
    "typescript": "~5.9.2",
    "vite": "^7.2.7",
    "vitest": "^4.0.16"
  },
  "private": true
}
